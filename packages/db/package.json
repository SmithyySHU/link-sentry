{
  "name": "@scanlark/db",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsx src/testConnection.ts",
    "demo:latest-scan": "node --import tsx src/printLatestScan.ts",
    "demo:site-history": "node --import tsx src/printSiteHistory.ts",
    "demo:schedule": "node --import tsx src/scheduleDemo.ts",
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "argon2": "^0.31.2",
    "dotenv": "^16.4.5",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0"
  }
}
